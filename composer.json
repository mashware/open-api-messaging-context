{
  "name": "mashware/open-api-messaging-context",
  "description": "Behat context for validating messages with OpenApi schema for symfony 2",
  "authors": [
    {
      "name": "Aarón Bernabeu Rodríguez",
      "email": "aaron.bernabeu@pccomponentes.com"
    },
    {
      "name": "Alberto Vioque Muñoz",
      "email": "alberto.vioque@pccomponentes.com"
    }
  ],
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "PcComponentes\\OpenApiMessagingContext\\": "src/"
    }
